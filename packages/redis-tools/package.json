{
  "name": "redis-tools",
  "version": "0.0.0",
  "description": "Redis tools for use with NodeJS",
  "keywords": [
    "redis"
  ],
  "repository": "https://github.com/bsantare/npt/redis-tools",
  "license": "MIT",
  "main": "build/main/index.js",
  "module": "build/module/index.js",
  "typings": "build/main/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "tsc",
    "build:check": "tsc --noEmit",
    "clean": "rimraf build/*",
    "fix:eslint": "eslint --fix \"**/*.{ts,tsx,js}\"",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "sort:packages": "npx sort-package-json",
    "test": "npm run test:unit",
    "test:unit": "jest"
  },
  "prettier": {
    "singleQuote": true
  },
  "dependencies": {
    "@types/redis": "^2.8.15",
    "@types/uuid": "^3.4.7",
    "reflect-metadata": "^0.1.13",
    "uuid": "^3.4.0",
    "winston": "^3.2.1"
  },
  "optionalDependencies": {
    "handy-redis": "^1.8.0",
    "redis": "^3.0.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
