{
  "name": "@node-power-tools/redis-tools",
  "version": "0.0.1-alpha.43",
  "description": "Redis tools for use with NodeJS",
  "keywords": [
    "redis"
  ],
  "repository": "https://github.com/bsantare/node-power-tools",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "build:check": "tsc --noEmit",
    "clean": "rimraf dist/*",
    "fix:eslint": "eslint --fix \"**/*.{ts,tsx,js}\"",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "publish": "yarn publish",
    "sort:packages": "npx sort-package-json",
    "test": "yarn test:unit",
    "test:unit": "jest"
  },
  "prettier": {
    "singleQuote": true
  },
  "dependencies": {
    "@js-joda/core": "^2.0.0",
    "@types/redis": "^2.8.15",
    "@types/uuid": "^3.4.7",
    "@types/verror": "^1.10.3",
    "reflect-metadata": "^0.1.13",
    "uuid": "^3.4.0",
    "winston": "^3.2.1",
    "verror": "^1.10.0"
  },
  "optionalDependencies": {
    "handy-redis": "^1.8.0",
    "redis": "^3.0.2"
  }
}
