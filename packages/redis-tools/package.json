{
  "name": "redis-tools",
  "version": "0.0.0",
  "description": "Redis tools for use with NodeJS",
  "keywords": [
    "redis"
  ],
  "repository": "https://github.com/bsantare/npt/redis-tools",
  "license": "MIT",
  "main": "build/main/index.js",
  "module": "build/module/index.js",
  "typings": "build/main/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "tsc",
    "build:check": "tsc --noEmit",
    "clean": "rimraf build/*",
    "fix:eslint": "eslint --fix \"**/*.{ts,tsx,js}\"",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "sort:packages": "npx sort-package-json",
    "test": "npm run test:unit",
    "test:unit": "jest"
  },
  "prettier": {
    "singleQuote": true
  },
  "dependencies": {
    "@types/redis": "^2.8.15",
    "@types/uuid": "^3.4.7",
    "reflect-metadata": "^0.1.13",
    "uuid": "^3.4.0",
    "winston": "^3.2.1"
  },
  "optionalDependencies": {
    "handy-redis": "^1.8.0",
    "redis": "^3.0.2"
  },
  "devDependencies": {
    "@types/jest": "^24.9.1",
    "@typescript-eslint/eslint-plugin": "^2.13.0",
    "@typescript-eslint/parser": "^2.13.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^23.2.0",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^25.1.0",
    "jest-mock-extended": "^1.0.8",
    "npm-ci": "^0.0.2",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "ts-jest": "^25.2.0",
    "typescript": "^3.7.5"
  },
  "publishConfig": {
    "access": "public"
  }
}
